#COMANDO BASICOS BASH

$ls # mostrar listado
	$ls -a
	$ls -l
$touch # crear archivo
$mkdir # crear directorio
$cd # ir a directorio
	$cd .. #para ir a la carpeta anterior
	$cd tab #para autocompletar
$rm #eliminar
	$rm -r # eliminar directorio
$pwd imprimir directorio actual

#SSH me define la compu de manera unica. 

CREAR UN REPOSITORIO
$git init    #inicializacion
$git branch -M main #cambiar nombre a main
$git remote add origin #"el repostory que cree"
$git add diff #lo que cambie
$git add . / "" # aÃ±adir a lo que debe ser confirmado
$git commit -m "" 
#puedo hacer los dos juntos :
$git commit -am  ""
$git push "origin" #llevarlo a GITHUB
$git log #ver los commits que hice
$git status 
$git checkout -b "rama_nueva_caracteristica" >> "" #los cambios en la rama no se reflejan en main
$git branch -a
$echo "Una nueva linea en rama_nueva_caracteristica" 
$git checkout main  #para cambiar de rama/main
#estando en un branch hago git push para llevarlo al repositorio local
#hago pull request para comparar y hacer merge.